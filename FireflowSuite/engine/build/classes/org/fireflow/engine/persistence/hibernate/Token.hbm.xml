<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="org.fireflow.kenel.impl.Token" table="T_FF_RT_TOKEN" dynamic-update="true">
		<id name="id" type="java.lang.String">
			<column name="ID" length="50"/>
			<generator class="uuid.hex"/>
		</id>
		<property name="alive" type="java.lang.Boolean">
			<column name="ALIVE" not-null="true"/>
		</property>
		<property name="value" type="java.lang.Integer">
			<column name="VALUE" not-null="true"/>
		</property>
		<property name="nodeId" type="java.lang.String">
			<column name="NODE_ID" not-null="true"/>
		</property>
		
		<many-to-one name="processInstance"
		    column="PROCESSINSTANCE_ID"
        	class="org.fireflow.engine.impl.ProcessInstance"
        	update="false"
        	not-null="true"
        	lazy="false"
        	cascade="none"/>
	</class>
</hibernate-mapping>