CREATE MEMORY TABLE T_FF_RT_PROCESSINSTANCE(ID VARCHAR(50) NOT NULL PRIMARY KEY,PROCESS_ID VARCHAR(100) NOT NULL,VERSION NUMERIC(10) NOT NULL,NAME VARCHAR(100),DISPLAY_NAME VARCHAR(128),STATE NUMERIC(10),CREATED_TIME TIMESTAMP,STARTED_TIME TIMESTAMP,EXPIRED_TIME TIMESTAMP,END_TIME TIMESTAMP,PARENT_PROCESSINSTANCE_ID VARCHAR(50),PARENT_TASKINSTANCE_ID VARCHAR(50))
