CREATE MEMORY TABLE T_FF_RT_TASKINSTANCE(ID VARCHAR(50) NOT NULL PRIMARY KEY,BIZ_TYPE VARCHAR(250) NOT NULL,TASK_ID VARCHAR(300) NOT NULL,ACTIVITY_ID VARCHAR(200) NOT NULL,NAME VARCHAR(100) NOT NULL,DISPLAY_NAME VARCHAR(128),STATE NUMERIC(10),TASK_TYPE VARCHAR(10),CREATED_TIME TIMESTAMP NOT NULL,STARTED_TIME TIMESTAMP,EXPIRED_TIME TIMESTAMP,END_TIME TIMESTAMP,ASSIGNMENT_STRATEGY VARCHAR(10) NOT NULL,PROCESSINSTANCE_ID VARCHAR(50) NOT NULL,PROCESS_ID VARCHAR(100) NOT NULL,VERSION NUMERIC(10) NOT NULL)
CREATE MEMORY TABLE T_FF_RT_TOKEN(ID VARCHAR(50) NOT NULL PRIMARY KEY,ALIVE NUMERIC(1) NOT NULL,VALUE NUMERIC(10) NOT NULL,NODE_ID VARCHAR(200) NOT NULL,PROCESSINSTANCE_ID VARCHAR(50) NOT NULL)
